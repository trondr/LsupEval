<SolutionConfiguration>
  <Settings>
    <AutoEnable>True</AutoEnable>
    <CurrentEngineMode>Run all tests manually [Global]</CurrentEngineMode>
    <HotSpotsTestsWindowTreeLayout>
      <Column>
        <Name>TotalExecutionTime</Name>
        <Width>332</Width>
      </Column>
      <Column>
        <Name>Location</Name>
        <Width>194</Width>
      </Column>
      <Column>
        <Name>LineText</Name>
        <Width>974</Width>
      </Column>
    </HotSpotsTestsWindowTreeLayout>
    <HotSpotsWindowSplitterDistance>397</HotSpotsWindowSplitterDistance>
    <TestsWindowGroupingStrategy>Project, Fixture, Test</TestsWindowGroupingStrategy>
    <TestsWindowMenuOptions>
      <VerticalSplitMenuOption>false</VerticalSplitMenuOption>
      <ShowPassingTestsMenuOption>false</ShowPassingTestsMenuOption>
      <ShowFailingTestsMenuOption>true</ShowFailingTestsMenuOption>
